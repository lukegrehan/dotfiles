alias gain='mp3gain -r *.mp3'
alias du='du -kh'
alias df='df -kh'
alias rm='rm -r'
alias cdtemp='cd $(mktemp --dir)'
alias pushtemp='pushd . && cdtemp'
alias chalias='nano ~/.bash_aliases'
alias title='mocp -Q%title'
alias mkreport='mkdir report && cd report && mkdir res && cp ~/Schoolwork/report_template.tex $(pwd)/report.tex'
alias shutdown='shutdown now'
alias sshuttle='sshuttle -NHr school'
alias clip='xclip -selection clipboard'
